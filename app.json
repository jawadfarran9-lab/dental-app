{
  "expo": {
    "name": "BeSmile AI",
    "slug": "dental-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "besmileai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.besmile.ai",
      "associatedDomains": [
        "applinks:besmile.app",
        "webcredentials:besmile.app"
      ],
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to let you add photos to your stories.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos for stories."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/android-icon-foreground.png",
        "backgroundImage": "./assets/android-icon-background.png",
        "monochromeImage": "./assets/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.besmile.ai",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "besmile.app",
              "pathPrefix": "/post/"
            },
            {
              "scheme": "besmileai",
              "host": "*"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://besmile.app"
        }
      ],
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to your photo library to let you add photos to your stories.",
          "cameraPermission": "This app needs access to your camera to take photos for stories."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    }
  }
}
